{
	"name": "@paraport/sdk",
	"version": "0.1.0-alpha.4",
	"description": "ParaPort Embedded UI",
	"keywords": ["paraport", "polkadot", "sdk"],
	"author": "KodaDot",
	"license": "MIT",
	"type": "module",
	"files": ["dist", "README.md"],
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"style": "./dist/index.css",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./style": "./dist/index.css"
	},
	"scripts": {
		"dev": "vite build --watch --mode development",
		"build": "vite build",
		"lint": "biome check --write ."
	},
	"dependencies": {
		"@paraport/vue": "workspace:*",
		"@paraport/core": "workspace:*",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vite-plugin-dts": "^4.5.0"
	}
}
