{
	"name": "@paraport/static",
	"version": "0.0.2-alpha.1",
	"description": "Static configuration and constants for ParaPort SDK",
	"keywords": ["paraport", "polkadot", "sdk", "blockchain"],
	"author": "KodaDot",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist", "README.md", "LICENSE"],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch --mode development",
		"prepare": "node scripts/generate-endpoints.mjs",
		"lint": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@polkadot/api": "11.2.1",
		"@polkadot/apps-config": "0.153.1",
		"@types/node": "^22.14.1",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vite-plugin-dts": "^4.5.0"
	}
}
